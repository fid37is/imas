{
  "name": "inventory-management-functions",
  "version": "1.0.0",
  "type": "module",
  "description": "Firebase Cloud Functions for Inventory Management System with Google Drive and Sheets integration",
  "main": "index.js",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--openssl-legacy-provider' next dev",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "build": "next build",
    "start": "NODE_OPTIONS='--openssl-legacy-provider' next start",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "local": "NODE_ENV=development node index.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.4",
    "cors": "^2.8.5",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1",
    "googleapis": "^110.0.0",
    "mime-types": "^3.0.1",
    "sharp": "^0.31.3"
  },
  "private": true
}
